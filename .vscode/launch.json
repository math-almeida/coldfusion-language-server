{
  "version": "2.0.0",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Extension",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/lib/js/client/src/*.js"
      ],
      "preLaunchTask": {
        "type": "npm",
        "script": "res:build"
      }
    }
  ]
}
